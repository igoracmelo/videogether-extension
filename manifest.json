{
  "manifest_version": 2,
  "name": "videogether",
  "version": "0.1.0",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "socket.io.min.js",
        "script.js"
      ]
    }
  ]
}